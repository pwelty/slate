# Tailscale Authentication Key
# Get your auth key from https://login.tailscale.com/admin/settings/keys
TS_AUTHKEY=tskey-auth-YOUR-KEY-HERE

# Dashboard theme (optional, defaults to 'dark')
# Available: dark, light, minimal-dark, retro, synthwave, tokyo-night, ocean
THEME=dark

# API Keys for widgets (optional)
TODOIST_API_TOKEN=
LINKWARDEN_API_KEY=
JOPLIN_API_KEY=
TRILIUM_URL=
TRILIUM_TOKEN=
OBSIDIAN_API_URL=
OBSIDIAN_API_URL_DOCKER=
OBSIDIAN_API_KEY=

# Pi-hole configuration (supports v5 and v6+)
PIHOLE_BASE_URL=https://pihole.example.com
PIHOLE_API_TOKEN=YOUR_APPLICATION_PASSWORD